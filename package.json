{
  "name": "obsidian-sample-plugin",
  "version": "4.0.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "check-types": "tsc --noemit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src/**/* --fix",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clean": "pnpm prettier && pnpm lint:fix",
    "test": "jest",
    "rlnotes": "git log $(git describe --tags --abbrev=0)..HEAD --oneline > release-notes.md && git add release-notes.md",
    "bump": "node version-bump.mjs && git add manifest.json versions.json package.json",
    "release": "git commit -m $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@babel/preset-typescript": "7.28.5",
    "@codemirror/language": "github:lishid/cm-language",
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/download": "8.0.5",
    "@types/jest": "30.0.0",
    "@types/node": "25.3.0",
    "@types/nunjucks": "3.2.6",
    "@types/react-dom": "19.2.3",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "babel-jest": "30.2.0",
    "builtin-modules": "5.0.0",
    "cross-env": "10.1.0",
    "esbuild": "0.27.3",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "7.37.5",
    "import-sort-cli": "6.0.0",
    "import-sort-style-module-alias": "1.1.0",
    "jest": "30.2.0",
    "moment": "2.30.1",
    "obsidian": "1.12.2",
    "prettier": "3.8.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "xml-js": "1.6.11"
  },
  "dependencies": {
    "download": "8.0.0",
    "escape-path-with-spaces": "1.0.2",
    "execa": "9.6.1",
    "fuse.js": "7.1.0",
    "nunjucks": "3.2.4",
    "preact": "10.28.4",
    "react": "npm:@preact/compat@^18.3.1",
    "react-dom": "npm:@preact/compat@^18.3.1",
    "react-json-tree": "0.20.0",
    "react-select": "5.10.2",
    "shell-path": "^3.1.0",
    "which": "^6.0.1",
    "zotero-api-client": "^0.48.0"
  }
}
